# Example .env file for Pit Stop Programmers Server
# Rename this to .env and fill in your actual secrets and configuration values

# Database
MONGODB_URI=mongodb://localhost:27017/pitstop_db

# JWT Secret
JWT_SECRET=your_jwt_secret

# Session Secret
SESSION_SECRET=your_session_secret

# Email Credentials
EMAIL_USER=your_email@example.com
EMAIL_PASS=your_email_password

# Server Port
PORT=5001
